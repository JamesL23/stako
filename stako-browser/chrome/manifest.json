{
  "manifest_version": 2,
  "name": "STAKO",
  "version": "0.1.0",
  "description": "STAKO stores your Stack Overflow visits, provides personalized statistics, and adds to your experience of Stack Overflow Q&A pages.",
  "key": "",
  "icons": {
    "16": "img/stako16.png",
    "48": "img/stako48.png",
    "128": "img/stako128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/stako16.png",
      "48": "img/stako48.png",
      "128": "img/stako128.png"
    },
    "default_popup": "popup.html",
    "default_title": "STAKO"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webNavigation",
    "identity",
    "identity.email",
    "https://stako.org/*",
    "https://stackoverflow.com/*"
  ],
  "oauth2": {
    "client_id": "504661212570-gn5nc6mtccuj7hsjcn419vjfapbq4f9a.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/userinfo.profile"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com/; object-src 'self'"
}
